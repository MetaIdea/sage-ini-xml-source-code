<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:FX_FlakShell.w3x" />
	</Includes>
	<GameObject
		id="JapanAntiAirShipWeaponProjectile"
		Side="Japan"
		EditorSorting="SYSTEM"
		KindOf="PROJECTILE SMALL_MISSILE DEFLECT_BY_SPECIAL_POWER NO_COLLIDE HIDE_IF_FOGGED"
		EditorName="JapanAntiAirShipWeaponProjectile">
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw">
        <ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
          <Model
						Name="FX_FlakShell" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="JapanMissile_Fire"
						FollowBone="true" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="ChopperMissileSmoke"
						FollowBone="true" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="JapanMissile_Light"
						FollowBone="true" />
        </ModelConditionState>
      </ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<FXListBehavior
				id="ModuleTag_FXLists">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_JapanMIRVHit" />
			</FXListBehavior>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				Flags="ORIENT_TO_FLIGHT_PATH DETONATE_CALLS_KILL PING_PONG_SIDEWAYS_DRIFT"
				FirstHeightMin="10"
				FirstHeightMax="20"
				SecondHeightMin="10"
				SecondHeightMax="20"
				FirstPercentIndent="33"
				SecondPercentIndent="66"
				GroundHitFX="FX_JapanMIRVHit"
				MaxDistanceReachedFX="FX_JapanMIRVHit"
				SidewaysDrift="50"
				FlightPathAdjustDistPerSecond="100.0"
				MaxDistanceToTravel="600.0"
				/>
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_ActiveBody"
				MaxHealth="100.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"></Shape>
		</Geometry>
		<VisionInfo
			VisionRange="0" />
	</GameObject>
  
  <!-- VETERAN -->
  
	<GameObject
		inheritFrom="JapanAntiAirShipWeaponProjectile"
		id="JapanAntiAirShipWeaponProjectile_Veteran"
		EditorName="JapanAntiAirShipWeaponProjectile_Veteran">
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw"
				xai:joinAction="Replace">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
          <Model
						Name="FX_FlakShell" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="JapanMissile_Fire_Vet"
						FollowBone="true" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="ChopperMissileSmoke"
						FollowBone="true" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="JapanMissile_Light_Vet"
						FollowBone="true" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				xai:joinAction="Replace"
				Flags="ORIENT_TO_FLIGHT_PATH DETONATE_CALLS_KILL PING_PONG_SIDEWAYS_DRIFT"
				FirstHeightMin="10"
				FirstHeightMax="20"
				SecondHeightMin="10"
				SecondHeightMax="20"
				FirstPercentIndent="33"
				SecondPercentIndent="66"
				GroundHitFX="FX_JapanMIRVHit_Vet"
				MaxDistanceReachedFX="FX_JapanMIRVHit_Vet"
				SidewaysDrift="50"
				FlightPathAdjustDistPerSecond="100.0"
				MaxDistanceToTravel="600.0"
				/>
		</Behaviors>
	</GameObject>	
</AssetDeclaration>