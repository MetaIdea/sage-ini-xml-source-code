		<Rule IfOriginalShaderIs="Lightning.fx" ReplaceShaderName="Lightning.fx" ReplaceTechniqueName="Default" />
		<Rule ReplaceShaderName="ObjectsIronCurtain.fx" ReplaceTechniqueName="Default" />
	</ShaderOverride>

