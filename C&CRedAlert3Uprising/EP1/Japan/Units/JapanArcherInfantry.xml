						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="B_bow_center" />
				</ModelConditionState>
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_1">
					<Model
						Name="NPSkeleton_SKN" />
				</ModelConditionState>
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
