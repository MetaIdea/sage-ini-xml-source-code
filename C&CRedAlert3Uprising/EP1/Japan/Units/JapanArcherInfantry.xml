					StateName="TRANS_fromSuppressed"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="AUCommandoTech1_SGTA"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- affected by bark or roar -->
				<AnimationState
