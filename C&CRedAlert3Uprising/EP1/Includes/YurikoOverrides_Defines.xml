<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
	</Includes>

<!-- Ranges Information
- 150 is right at the screen edge with the camera angle we have
- Was 100/125/150
-->	

	<Defines>
		<Define name="SHORT_RANGE_WEP" value="90.0" />
		<Define name="MED_RANGE_WEP" value="110.0" />
		<Define name="LONG_RANGE_WEP" value="150.0" />
		<Define name="EXTREME_RANGE_WEP" value="300.0" />
		
		<Define name="SHORT_RANGE_VIS" value="=$SHORT_RANGE_WEP*2.0" />
		<Define name="MED_RANGE_VIS" value="=$MED_RANGE_WEP*2.0" />
		<Define name="LONG_RANGE_VIS" value="=$LONG_RANGE_WEP*1.5" />
		<Define name="EXTREME_RANGE_VIS" value="=$EXTREME_RANGE_WEP*1.25" />

		<Define name="EGG_UNPACK_TIME" value="5s" />

		<Define name="SHORT_BUILD_TIME" value="3" />
		<Define name="MED_BUILD_TIME" value="6" />
		<Define name="LONG_BUILD_TIME" value="9" />
	</Defines>
	
</AssetDeclaration>
