<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:FXInvisBone.w3x" />
	</Includes>
	<!-- CPDummyWidget is a non-attackable, non-selectable object for use in playing sounds or as an alternative to waypoints. -->
	<!-- The USER_1 model condition is there so you can give all your CPDummyWidgets the USER_1 model condition if you want to see them in-game. -->
	<GameObject
		id="CPDummyWidget"
		Side="CIVILIAN"
		KindOf="INERT NOT_AUTOACQUIRABLE IMMOBILE"
		EditorName="CPDummyWidget"
		EditorSorting="SYSTEM">
		<DisplayName>CPDummyWidget</DisplayName>
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="FXInvisBone" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="WORLD_BUILDER">
					<Model
						Name="locator" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_1">
					<Model
						Name="locator" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<DestroyDie
				id="ModuleTag_DestroyDie">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
		</Behaviors>
		<Body>
			<PropBody
				id="ModuleTag_Body"/>
		</Body>
	</GameObject>
</AssetDeclaration>