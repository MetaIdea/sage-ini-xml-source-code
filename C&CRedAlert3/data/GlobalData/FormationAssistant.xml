<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset">

	<!-- Unit Definitions. A set of ObjectFilters to describe each type of unit -->

	<FormationAssistantUnitDefinition id="Armor">
		<IncludeObjectFilter
			Rule="ANY"
			Include="VEHICLE"
			Exclude="AIRCRAFT SIEGE_WEAPON FS_FACTORY HARVESTER MCV EXPANSION_UNIT"
			StatusBitFlagsExclude="TRANSFORMATION_TOGGLE_STATE">
			<ExcludeThing>SovietAntiAirShip</ExcludeThing>
			<ExcludeThing>SovietAntiAirShip_Ground</ExcludeThing>
			<ExcludeThing>AlliedAntiAirVehicleTech1</ExcludeThing>
			<ExcludeThing>AlliedAntiAirShip</ExcludeThing>
			<ExcludeThing>JapanAntiAirVehicleTech1</ExcludeThing>
			<ExcludeThing>JapanAntiAirShip</ExcludeThing>
		</IncludeObjectFilter>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Infantry">
		<IncludeObjectFilter
			Rule="NONE"
			Include="INFANTRY"
			Exclude="CAN_CAPTURE ENGINEER"
		/>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Support">
		<IncludeObjectFilter
			Rule="NONE"
			Include="VEHICLE"
			Exclude="SIEGE_WEAPON FS_FACTORY HARVESTER MCV EXPANSION_UNIT EGG">
		</IncludeObjectFilter>
	</FormationAssistantUnitDefinition>

	<FormationAssistantUnitDefinition id="Artillery">
		<IncludeObjectFilter
			Rule="NONE"
			Include="SIEGE_WEAPON"
		/>
	</FormationAssistantUnitDefinition>

	<!-- Including aircraft with support units for now
  <FormationAssistantUnitDefinition id="NonCombatant">
		<IncludeObjectFilter
			Rule="NONE"
			Include="AIRCRAFT"
		/>
	</FormationAssistantUnitDefinition>
   -->

	<FormationAssistantUnitDefinition id="NonCombatant">
		<IncludeObjectFilter
			Rule="ANY"
			Include="CAN_CAPTURE HARVESTER MCV EXPANSION_UNIT ENGINEER EGG"
		/>
	</FormationAssistantUnitDefinition>

