<?xml version="1.0" encoding="us-ascii"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xai="uri:ea.com:eala:asset:instance">
	<Tags></Tags>
	<Includes>
		<Include
			type="all"
			source="ART:SUAntiStructureVehicle_Missle.w3x" />
		<Include
			type="all"
			source="ART:FXNodTankShell.w3x" />
	</Includes>
	<GameObject
		id="SovietAntiStructureVehicleV4RocketProjectile"
		Side="Soviet"
		EditorSorting="SYSTEM"
		KindOf="PROJECTILE SMALL_MISSILE DEFLECT_BY_SPECIAL_POWER NO_COLLIDE HIDE_IF_FOGGED"
		EditorName="SovietAntiStructureVehicleV4RocketProjectile">
		<ArmorSet
			Armor="NoArmor" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="SUAntiStructureVehicle_Missle" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="SovietShipV4TrailFire"/>
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="SovietShipV4TrailFireLight"/>
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="SovietShipV4TrailSmoke"/>
          
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<LifetimeUpdate
			  id="ModuleTag_LifetimeUpdate"
			  MinLifetime="1.0s"
			  MaxLifetime="1.0s" />
      <FXListBehavior
				id="ModuleTag_FXLists">
        <DieMuxData
					DeathTypes="ALL" />
        <Event
					Index="onDeath"
					FX="FX_SOV_V4Explosion" />
      </FXListBehavior>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>
			<BezierProjectile
				id="ModuleTag_BezierProjectileBehavior"
				Flags="ORIENT_TO_FLIGHT_PATH DETONATE_CALLS_KILL"
				FirstHeightMin="350"
				FirstHeightMax="400"
				SecondHeightMin="350"
				SecondHeightMax="400"
				FirstPercentIndent="0"
				SecondPercentIndent="30"
				GroundHitFX="FX_SOV_V4Explosion"
				FlightPathAdjustDistPerSecond="10.0"
				Acceleration="0.25"
				/>
		</Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_ActiveBody"
				MaxHealth="100.0" />
		</Body>
		<Geometry
			IsSmall="true">
			<Shape
				Type="SPHERE"
				MajorRadius="0.8"/>
		</Geometry>
		<AudioArraySound>
			<AudioEntry Sound="SOV_V4MissileLauncher_MissileIncoming" AudioType="soundAmbient" />
		</AudioArraySound>
		<VisionInfo
			VisionRange="0" />
	</GameObject>

  <!-- VETERAN -->

  <GameObject
    inheritFrom="SovietAntiStructureVehicleV4RocketProjectile"
		id="SovietAntiStructureVehicleV4RocketProjectile_Veteran">
    <Draws>
      <ScriptedModelDraw
				id="ModuleTag_ScriptedModelDraw"
        xai:joinAction="Replace">
        <ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
          <Model
						Name="SUAntiStructureVehicle_Missle" />
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="SovietShipV4TrailFire_Vet"/>
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="SovietShipV4TrailFireLight_Vet"/>
          <ParticleSysBone
						BoneName="NONE"
						FXParticleSystemTemplate="SovietShipV4TrailSmoke"/>
        </ModelConditionState>
      </ScriptedModelDraw>
    </Draws>
    <Behaviors>
      <FXListBehavior
				id="ModuleTag_FXLists"
        xai:joinAction="Replace">
        <DieMuxData
					DeathTypes="ALL" />
        <Event
					Index="onDeath"
					FX="FX_SOV_V4Explosion_Vet" />
      </FXListBehavior>
    </Behaviors>
  </GameObject>
</AssetDeclaration>