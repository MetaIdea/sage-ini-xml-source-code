		</EvaEventArray>
		
		<VisionInfo
			VisionRange="200"
			ShroudClearingRange="=$STANDARD_SHROUD_CLEAR" />
		<CrusherInfo
			id="id_CrusherInfo"
			CrusherLevel="30"
			CrushableLevel="31" 
			CrushWeapon="SovietCrushWeapon"
			CrushDecelerationPercent="80%">
		<ExtraCrushLevels CrusherLevel="1" CrushableLevel="31" ModelConditionMatch="OVER_WATER"/>	
		 </CrusherInfo>
	</GameObject>
	
	
