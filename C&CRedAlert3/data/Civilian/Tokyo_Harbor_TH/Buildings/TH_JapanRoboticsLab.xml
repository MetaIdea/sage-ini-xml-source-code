		<Include type="instance" source="DATA:Civilian/Yucatan_YU/Props/YU_HotelDebris.xml" />
		<Include type="instance" source="DATA:Civilian/Tokyo_Harbor_TH/Props/TH_JapanRoboticsLab_Collapse.xml" />
	</Includes>
  <GameObject
		id="TH_JapanRoboticsLab"
		inheritFrom="BaseCivilianStructureNoGarrisonDestructible"
		EditorName="TH_JapanRoboticsLab"	
    	EditorSorting="STRUCTURE"
		Browser="REGION Tokyo_Harbor_TH STRUCTURE"
    	KindOf="+FS_FACTORY +AUTO_RALLYPOINT +FS_BARRACKS +FS_WAR_FACTORY -CIVILIAN_BUILDING"
    	CommandSet="CAMPA07SpecialBuildsCommandSet"
		SelectPortrait="Portrait_JapanRoboticsLab"
		ButtonImage="Button_JapanRoboticsLab"
		TypeDescription="TYPE:JapanRoboticsLab"
		Description="DESC:JapanRoboticsLab">
		<DisplayName xai:joinAction="Replace">Name:JapanRoboticsLab</DisplayName>
	<ArmorSet
      id="DefaultArmor"
      xai:joinAction="Replace" 
      Armor="A07_JapanRobotLabArmor"
      DamageFX="GenericStructureDamageFX" />   
    <Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true"
				ExtraPublicBone="BONE_CONTACT_POINT_01 BONE_CONTACT_POINT_02 BONE_CONTACT_POINT_03 BONE_CONTACT_POINT_04 BONE_CONTACT_POINT_05 BONE_CONTACT_POINT_06 BONE_CONTACT_POINT_07 BONE_CONTACT_POINT_08 BONE_CONTACT_POINT_09 BONE_CONTACT_POINT_10 BONE_CONTACT_POINT_11 BONE_CONTACT_POINT_12 BONE_CONTACT_POINT_13 BONE_CONTACT_POINT_14 BONE_CONTACT_POINT_15 BONE_CONTACT_POINT_16 BONE_CONTACT_POINT_17 BONE_CONTACT_POINT_18 BONE_CONTACT_POINT_19 BONE_CONTACT_POINT_20">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="TH_JapanRoboticsLab" />
				</ModelConditionState>
			</ScriptedModelDraw>
		</Draws>
		<Behaviors>
			<Physics
				id="ModuleTag_Physics"/>

			<BuildingDestructionBehavior
				id="ModuleTag_BuildingDestruction">
				<DieMuxData
					DeathTypes="ALL" />
				<Item
					BonePrefix="BONE_CONTACT_POINT_"
					FX="FX_BuildingDebrisExplosion"
					CreationObject="YU_HotelDebris"
					FlingVelocity="100.0"/>
			</BuildingDestructionBehavior>

			<FXListBehavior
				xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance" id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL" />
				<Event
					Index="onDeath"
					FX="FX_Civ_Explosion_Medium" />
			</FXListBehavior>

			<CreateObjectDie
				id="ModuleTag_CreateObjectDie"
				CreationList="TH_JapanRoboticsLab_Die_OCL">
				<DieMuxData
					DeathTypes="ALL" />
			</CreateObjectDie>	
			
			<!-- civ buildings should have a CreateObjectDie to make a collapse version. -->
			<DestroyDie
				id="ModuleTag_DestroyDie">
				<DieMuxData
					DeathTypes="ALL" />
			</DestroyDie>

      <ProductionUpdate
        id="ModuleTag_ProductionUpdate"
        GiveNoXP="true"
        Type="INFANTRY"
        NumDoorAnimations="1"
        DoorOpeningTime="0.66s"
        DoorWaitOpenTime="1.0s"
        DoorCloseTime="0.66s">
        <GiveUpgradeToProduced>
          <UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_VETERAN"
						ProvidedUpgrade="Upgrade_CreationVeterancy_VETERAN" />
          <UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_ELITE"
						ProvidedUpgrade="Upgrade_CreationVeterancy_ELITE" />
          <UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_HEROIC"
						ProvidedUpgrade="Upgrade_CreationVeterancy_HEROIC" />
        </GiveUpgradeToProduced>
      </ProductionUpdate>      
      <ProductionUpdate
        id="ModuleTag_ProductionUpdate2"
        GiveNoXP="true"
        Type="VEHICLE"
        NumDoorAnimations="1"
        DoorOpeningTime="0.66s"
        DoorWaitOpenTime="1.0s"
        DoorCloseTime="0.66s">
        <GiveUpgradeToProduced>
          <UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_VETERAN"
						ProvidedUpgrade="Upgrade_CreationVeterancy_VETERAN" />
          <UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_ELITE"
						ProvidedUpgrade="Upgrade_CreationVeterancy_ELITE" />
          <UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_HEROIC"
						ProvidedUpgrade="Upgrade_CreationVeterancy_HEROIC" />
        </GiveUpgradeToProduced>
      </ProductionUpdate>
      <QueueProductionExitUpdate
				id="ModuleTag_ExitUpdate"
				PlacementViewAngle="45d"
				ExitDelaySeconds="0.07s">
        <UnitCreatePoint
					x="0.0"
					y="-20.0" />
        <NaturalRallyPoint
					x="0.0"
					y="-50.0" />
      </QueueProductionExitUpdate>

      <SubObjectsUpgrade
				id="ModuleTag_SubObjectsUpgrade">
        <TriggeredBy>Upgrade_JapanBarracksTech2</TriggeredBy>
        <Nugget
					Upgrade="Upgrade_JapanWarFactoryTech2"/>
      </SubObjectsUpgrade>
      <SubObjectsUpgrade
				id="ModuleTag_SubObjectsUpgrade2">
        <TriggeredBy>Upgrade_JapanBarracksTech3</TriggeredBy>
        <Nugget
					Upgrade="Upgrade_JapanWarFactoryTech3"/>
      </SubObjectsUpgrade>
      
      <SubObjectsUpgrade
				id="ModuleTag_SubObjectsUpgrade">
        <TriggeredBy>Upgrade_JapanWarFactoryTech2</TriggeredBy>
        <Nugget
					Upgrade="Upgrade_JapanWarFactoryTech2"/>
      </SubObjectsUpgrade>
      <SubObjectsUpgrade
				id="ModuleTag_SubObjectsUpgrade2">
        <TriggeredBy>Upgrade_JapanWarFactoryTech3</TriggeredBy>
        <Nugget
					Upgrade="Upgrade_JapanWarFactoryTech3"/>
      </SubObjectsUpgrade>
      
    </Behaviors>
		<Body>
			<ActiveBody
				id="ModuleTag_Body"
				MaxHealth="6000"
				/>
		</Body>
		<Geometry
			IsSmall="false">
<Shape 
     Type= "BOX"
     MajorRadius = "56.5283"
     MinorRadius = "82.5849"
     Height = "76.6028"
     ContactPointGeneration = "STRUCTURE" >
    <Offset
        x = "-1.9486"
        y = "-0.00150299"
        z = "0.0" />
</Shape>
		</Geometry>
		<AudioArraySound>
			<AudioEntry Sound="HumanFaction_MediumBuilding_LightDamageMS" AudioType="soundOnDamaged" />
			<AudioEntry Sound="HumanFaction_MediumBuilding_HeavyDamageMS" AudioType="soundOnReallyDamaged" />
		</AudioArraySound> 
	</GameObject>

	<ObjectCreationList
		id="TH_JapanRoboticsLab_Die_OCL">
		<CreateObject
			Options="IGNORE_ALL_OBJECTS"
			Disposition="LIKE_EXISTING RANDOM_FORCE"
			Count="1"
			MinForceMagnitude="1.0"
			MaxForceMagnitude="2.0"
			MinForcePitch="90d"
			MaxForcePitch="75d">
			<CreateObject>TH_JapanRoboticsLab_Collapse</CreateObject>
		</CreateObject>
	</ObjectCreationList>	
