	<GameObject
		id="JapanAntiVehicleInfantry"
		inheritFrom="BaseInfantry"
		SelectPortrait="Portrait_JapanAntiVehicleInfantry"
		ButtonImage="Button_JapanAntiVehicleInfantry"
		Side="Japan"
		SubGroupPriority="345"
		EditorSorting="UNIT"		
		BuildTime="5"
		HealthBoxHeightOffset="30"
		CommandSet="JapanAntiVehicleInfantryCommandSet"
		KindOf="SELECTABLE CAN_ATTACK CAN_CAST_REFLECTIONS INFANTRY SCORE"
		ThreatLevel="10"
		WeaponCategory="MISSILE"
		VoicePriority="100"
		EditorName="JapanAntiVehicleInfantry"
