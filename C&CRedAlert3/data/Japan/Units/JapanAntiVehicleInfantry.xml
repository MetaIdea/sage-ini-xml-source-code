			Conditions="IN_SPIDER_HOLE" />
		<LocomotorSet
			Locomotor="TestReactiveLocomotorHUMAN"
			Condition="NORMAL"
			Speed="50" />
		<SkirmishAIInformation
			UnitBuilderStandardCombatUnit="true" />
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="JUAntiVehicleInfantry_SKN" />
					<WeaponFireFXBone
						WeaponSlotID="1"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="fx01" />
					<WeaponLaunchBone
						WeaponSlotID="1"
						WeaponSlotType="PRIMARY_WEAPON"
						BoneName="fx01" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="INSIDE_GARRISON">
					<Model
						Name="" />
				</ModelConditionState>	
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_1">
					<Model
						Name="NPSkeleton_SKN" />
				</ModelConditionState>		
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
					<Model
						Name="NPRadiation" />
						<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="RadiationPuddle"
						FollowBone="true" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="RadiationSplash"
            FollowBone="true" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="RadiationLightSmall"
            FollowBone="true" />          
				</ModelConditionState>	

        	<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					StateName="STATE_bored"
					AllowRepeatInRandomPick="true"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIDA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIDB"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIDC"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFB"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFC"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFW"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFX"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFY"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_BIFZ"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10" 
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_inHole" then CurDrawableSetTransitionAnimState("TRANS_InHoleToStanding") end;
						if Prev == "STATE_Suppressed" then CurDrawableSetTransitionAnimState("TRANS_fromSuppressed") end;
						if Prev == "STATE_Selected" then CurDrawableSetTransitionAnimState("TRANS_SelectedToIdle") end;
						if Prev == "STATE_stunned_flailing" then CurDrawableSetTransitionAnimState("STATE_stunned_landing")	end;
						if Prev == "STATE_Parachuting" then CurDrawableSetTransitionAnimState("TRANS_fromParachuting") end;
					</Script>
				</AnimationState>     
				<!--START OF IN_SPIDER_HOLE-->
				
				<!-- called when summoned from a special power. -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING SPECIAL_POWER_1">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CDIEA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SPECIAL_POWER_1">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CRCHB"
						AnimationMode="ONCE_BACKWARDS"
						AnimationBlendTime="0"
						AnimationAbsoluteTime="10s"/>
					<FXEvent
						Frame="13"
						FireWhenSkipped="true"
						Effect="FX_Japan_Burrow" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING USER_6">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CDIEA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_CDIEB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- Idle animation of guy in the spider hole -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="USER_6"
					StateName="STATE_inHole"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CAIDA"
						AnimationMode="ONCE" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_bored" then
						CurDrawableSetTransitionAnimState("TRANS_StandingToInHole")
						elseif Prev == "STATE_Selected" then
						CurDrawableSetTransitionAnimState("TRANS_StandingToInHole")
						end
					</Script>
				</AnimationState>
				<!--END OF IN_SPIDER_HOLE-->

				<!-- Transition of guy going into the spider hole -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="UNPACKING">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CRCHB"
						AnimationMode="ONCE"/>
<!--
					<Script>
						CurDrawablePlaySound("JAP_Tankbuster_BurrowInMS")
					</Script>
-->
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="JUAntiVehicleInfantry_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					StateName="STATE_stunned_landing">
					<Animation
						AnimationName="JUAntiVehicleInfantry_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_STANDING_UP">
					<Animation
						AnimationName="JUAntiVehicleInfantry_GTPA"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FRONTCRUSHED">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CDTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BACKCRUSHED">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CDTB"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<!-- parachuting anims -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="LAUNCHED"
					StateName="STATE_ParachuteLaunch">
					<Animation
						AnimationName="JUAntiVehicleInfantry_MCNA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="PARACHUTING"
					StateName="STATE_Parachuting">
					<Animation
						AnimationName="JUAntiVehicleInfantry_MCNC"
						AnimationMode="LOOP" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_ParachuteLaunch" then
						CurDrawableSetTransitionAnimState("TRANS_toParachuting")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_toParachuting" >
					<Animation
						AnimationName="JUAntiVehicleInfantry_MCNB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_fromParachuting" >
					<Animation
						AnimationName="JUAntiVehicleInfantry_MCND"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED DYING"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="JUAntiVehicleInfantry_SDIA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<!-- radiation death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
					<Animation
						AnimationName="NPRadiation_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>

<!-- sword death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_4">
					<Animation
						AnimationName="JUAntiVehicleInfantry_DTKA"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- fire death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_3">
					<Animation
						AnimationName="JUAntiVehicleInfantry_DTFA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUAntiVehicleInfantry_DTFB"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="HIPS"
						FXParticleSystemTemplate="InfantryFireDeath"
						FollowBone="true" />
				</AnimationState>

				<!-- prism/laser death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_2">
					<Animation
						AnimationName="JUAntiVehicleInfantry_DTPA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUAntiVehicleInfantry_DTPB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- tesla death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_1">
					<Animation
						AnimationName="NPSkeleton_DIEA"
						AnimationMode="ONCE" />
          <ParticleSysBone
						BoneName="HIPS"
						FXParticleSystemTemplate="Soviet_TeslaDeath_Spark"
						FollowBone="true" />
				</AnimationState>

				<!-- dying when running -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING_WASMOVING">
					<Animation
						AnimationName="JUAntiVehicleInfantry_RNDA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="JUAntiVehicleInfantry_RNDB"
						AnimationMode="ONCE" />
				</AnimationState>
				
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="JUAntiVehicleInfantry_DTBA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_DTBB"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="AFFECTED_BY_EMP">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CWRY"
						AnimationMode="LOOP" />
					<Animation
						AnimationName="JUAntiVehicleInfantry_CWRZ"
						AnimationMode="LOOP" />
					<ParticleSysBone
							BoneName="NONE"
							FXParticleSystemTemplate="Allied_DogStun01"
							FollowBone="true" />
					<ParticleSysBone
							BoneName="NONE"
							FXParticleSystemTemplate="Allied_DogStunGlows01"
							FollowBone="true" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED MOVING"
					StateName="STATE_Suppressed"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="JUAntiVehicleInfantry_SMVA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED USING_WEAPON_A"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="JUAntiVehicleInfantry_SATA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="JUAntiVehicleInfantry_SIDA"
						AnimationMode="ONCE" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev ~= "STATE_Suppressed" and Prev ~= "TRANS_toSuppressed" then CurDrawableSetTransitionAnimState("TRANS_toSuppressed")	end;
					</Script>
				</AnimationState>

				<!-- Transition of guy standing up from spider hole -->
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_StandingToInHole">
					<Animation
						AnimationName="JUAntiVehicleInfantry_CRCHB"
