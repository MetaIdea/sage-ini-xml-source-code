<?xml version="1.0" encoding="utf-8"?>
<AssetDeclaration xmlns="uri:ea.com:eala:asset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xi="http://www.w3.org/2001/XInclude">
	<Tags></Tags>
	<!-- aka Seaport -->
	<Includes>
		<Include type="all" source="DATA:GlobalData/GlobalDefines.xml" /> 

		<Include type="all"	source="DATA:GlobalData/GlobalDefines.xml" />
		<Include type="all"	source="ART:ABNavalYard.w3x" />				
		<Include type="all"	source="ART:ABNavalYard_FX.w3x" />				
		<Include type="all"	source="ART:ABNavalYard_BLD.w3x" />				
		<Include type="all"	source="ART:ABNavalYard_Gag.w3x" />		
		<Include type="all"	source="ART:ABNavalYard_UPGRADE.w3x" />	
		<Include type="all"	source="ART:ABNavalYard_LIGHTS.w3x" />
		<Include type="all"	source="ART:FXInvisBone.w3x" />
		<Include type="all"	source="ART:Decal_Rig_CO.xml" />
		<!-- Base Object -->
    <Include type="instance"	source="DATA:BaseObjects/BaseStructure.xml" />
	</Includes>
	<GameObject
		id="AlliedNavalYard"
		inheritFrom="BaseStructure"
		SelectPortrait="Portrait_AlliedNavalYard"
		ButtonImage="Button_AlliedNavalYard_on"
		Side="Allies"
		EditorSorting="STRUCTURE"
		
		BuildTime="20"
		HealthBoxHeightOffset="50"
		EnergyProduction="-50"
		CommandSet="AlliedNavalYardCommandSet"
		KindOf="+FS_FACTORY +AUTO_RALLYPOINT +FS_WAR_FACTORY +CAN_NOT_BUILD_ON_LAND"
		RadarPriority="STRUCTURE"
		PlacementViewAngle="-45d"
		ThreatLevel="1.0"
		ProductionQueueType="MAIN_STRUCTURE"
		BuildPlacementTypeFlag="MAIN_STRUCTURE"
		EditorName="AlliedNavalYard"
		Description="Desc:AlliedNavalYard"
		TypeDescription="Type:AlliedNavalYard">
		<DisplayName
			xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance">Name:AlliedNavalYard</DisplayName>
		<GameDependency>
			<RequiredObject>AlliedRefinery</RequiredObject>
		</GameDependency>
		<ObjectResourceInfo>
			<BuildCost Account="=$ACCOUNT_ORE" Amount="1000"/>
		</ObjectResourceInfo>
		<ArmorSet
			Armor="AlliedWarFactoryArmor"
			DamageFX="FactionStructureDamageFX" />
		<SkirmishAIInformation
			BaseBuildingLocation="FRONT">
      <ConstructionBaseSelectionCriteria
        ExcludedBaseTypes="NO_BUILD_RADIUS CAPTURED"
        PreferredBaseTypes="MAIN_BASE"
        SortOrder="PREFER_OLDEST" />
    </SkirmishAIInformation>
		<Draws>
			<ScriptedModelDraw
				id="ModuleTag_Draw"
				OkToChangeModelColor="true"
				ExtraPublicBone="BONE_CONTACT_POINT_01 BONE_CONTACT_POINT_02 BONE_CONTACT_POINT_03 BONE_CONTACT_POINT_04 BONE_CONTACT_POINT_05 BONE_CONTACT_POINT_06 BONE_CONTACT_POINT_07 BONE_CONTACT_POINT_08 BONE_CONTACT_POINT_09 BONE_CONTACT_POINT_10"
				StaticModelLODMode="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABNavalYard" />
				</ModelConditionState>
        
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Model
						Name="ABNavalYard_BLD" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="ABPowerPlantWakeSplash"
						FollowBone="true" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SOLD">
					<Model
						Name="ABNavalYard_BLD" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="ABPowerPlantWakeSplash"
						FollowBone="true" />
				</ModelConditionState>				
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<!-- no anim -->
				</AnimationState>
				<!-- BUILDUP ANIMATION -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Animation
						AnimationName="ABNavalYard_BLD"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="AlliedPowerPlantWaterBuildupFroth"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="AlliedPowerPlantWaterBuildupMist"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="AlliedPowerPlantBuildupFlashes"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="AlliedPowerPlantBuildupSparksSeed"
						FollowBone="true" />          
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SOLD"
					Flags="START_FRAME_LAST">
					<Animation
						AnimationName="ABNavalYard_BLD"
						AnimationMode="ONCE_BACKWARDS"
						AnimationAbsoluteTime="2s"/>
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="AlliedPowerPlantWaterBuildupFroth"
						FollowBone="true" />
					<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="AlliedPowerPlantWaterBuildupMist"
						FollowBone="true" />
				</AnimationState>					

			</ScriptedModelDraw>
			
			<!-- DRAW GAG -->

			<ScriptedModelDraw
				id="ModuleTag_Draw_Gag"
				OkToChangeModelColor="true"
				StaticModelLODMode="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABNavalYard_Gag" />
				</ModelConditionState>

				<ModelConditionState
							ParseCondStateType="PARSE_NORMAL"
							ConditionsYes="FROZEN">
					<Model
								Name="ABNavalYard_Gag" />
					<Material
							ShaderName="ObjectsFrozen.fx"
							TechniqueName="Default">
					<Constants>
					<Texture Name="DiffuseTexture">
						<Value>ABNavalYard</Value>
					</Texture>
					<Texture Name="NormalMap">
						<Value>ABNavalYard_NRM</Value>
					</Texture>
					<Texture Name="SpecMap">
						<Value>ABNavalYard_SPM</Value>
					</Texture>
					<Float Name="EnvMult">
						<Value>1.000000</Value>
					</Float>
					<Bool Name="AlphaTestEnable">
						<Value>false</Value>
					</Bool>
					</Constants>
				</Material>
				</ModelConditionState>        
        
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Model
						Name="" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SOLD">
					<Model
						Name="" />
				</ModelConditionState>	
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ACTIVELY_CONSTRUCTING">
					<Animation
						AnimationName="ABNavalYard_GAG"
						AnimationMode="LOOP"
						AnimationBlendTime="90"  />
				</AnimationState>				
			</ScriptedModelDraw>
			
			<!-- Lights-->
			<ScriptedModelDraw
				id="ModuleTag_Draw_LIGHTS"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABNavalYard_Lights" />
				</ModelConditionState>
				
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT">
					<Script>
					CurDrawableHideSubObjectPermanently("FX_Lights01")
					</Script>
				</AnimationState>

        <AnimationState
            ParseCondStateType="PARSE_NORMAL"
            ConditionsYes="AFFECTED_BY_EMP">
          <Script>
            CurDrawableHideSubObjectPermanently("FX_Lights01")
          </Script>
        </AnimationState>
        <AnimationState
            ParseCondStateType="PARSE_NORMAL"
            ConditionsYes="PARALYZED">
          <Script>
            CurDrawableHideSubObjectPermanently("FX_Lights01")
          </Script>
        </AnimationState>
        
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ACTIVELY_CONSTRUCTING">
					<ParticleSysBone
						BoneName="FX_Steam01"
						FXParticleSystemTemplate="AlliedRefinerySteam"
						FollowBone="true" />
						<ParticleSysBone
						BoneName="FX_Steam02"
						FXParticleSystemTemplate="AlliedRefinerySteam"
						FollowBone="true" />
					<Script>
					CurDrawableShowSubObjectPermanently("FX_Lights01")
					</Script>
				</AnimationState>
			</ScriptedModelDraw>

<!-- DRAW UPGRADE -->
			<ScriptedModelDraw
				id="ModuleTag_Draw_UPGRADE"
				ExtraPublicBone="FX_BONE_UPGRADE_DISH">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABNavalYard_UPGRADE" />
				</ModelConditionState>
        <ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BUILD_PLACEMENT_CURSOR">
          <Model
						Name="" />
        </ModelConditionState>
        <ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STRUCTURE_UNPACKING">
					<Model
						Name="" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SOLD">
					<Model
						Name="" />
				</ModelConditionState>						
			</ScriptedModelDraw>
			
<!-- DRAW PARTICLES -->

      <ScriptedModelDraw
				id="ModuleTag_Draw_FX"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model
						Name="ABNavalYard_FX" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DAMAGED">
					<Model
						Name="ABNavalYard_FX" />
					<ParticleSysBone 
						BoneName="FX_BONE01" 
						FXParticleSystemTemplate="GenericDStateSmoke"
						FollowBone="true"/>
					<ParticleSysBone 
						BoneName="FX_BONE01"
 						FXParticleSystemTemplate="GenericDStateFire"
						FollowBone="true"/>
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="REALLYDAMAGED">
					<Model
						Name="ABNavalYard_FX" />
					<ParticleSysBone 
						BoneName="FX_BONE01" 
						FXParticleSystemTemplate="GenericDStateSmoke"
						FollowBone="true"/>
					<ParticleSysBone 
						BoneName="FX_BONE01"
 						FXParticleSystemTemplate="GenericDStateFire"
						FollowBone="true"/>
					<ParticleSysBone 
						BoneName="FX_BONE02" 
						FXParticleSystemTemplate="GenericDStateSmoke"
						FollowBone="true"/>
					<ParticleSysBone 
						BoneName="FX_BONE02"
 						FXParticleSystemTemplate="GenericDStateFire"
						FollowBone="true"/>

				</ModelConditionState>
			</ScriptedModelDraw>						

			<ScriptedModelDraw
				id="ModuleTag_RepairDecal"
				OkToChangeModelColor="true">
				<ModelConditionState
					ParseCondStateType="PARSE_DEFAULT">
					<Model Name="" />
				</ModelConditionState>
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SELECTED">
					<Model Name="FXInvisBone" />
					<ShadowInfo
						Type="ADDITIVE_DECAL"
						SizeX="=$REPAIR_DRONE_HOME_DECAL_SIZE"
						SizeY="=$REPAIR_DRONE_HOME_DECAL_SIZE"
						OverrideLODVisibility="true"
						MaxHeight="200"
						UseHouseColor="true"
						Texture="Decal_Rig_CO"
						LocalPlayerOnly="true"
					/>
				</ModelConditionState>
			</ScriptedModelDraw>				
		</Draws>
		<Behaviors>
			<AssignSlavesTargetObjectSpecialPower
				id="ModuleTag_SpecialPowerRepairVehicle"
				SpecialPowerTemplate="SpecialPower_TargetedRepairVehicle" />
			<SpawnBehavior
				id="ModuleTag_SpawnRepairDrones"
				SpawnNumberData="3"
				InitialBurst="3"
				SpawnReplaceDelayData="10s"
				SpawnedRequireSpawner="true"
				KillSpawnsOnCaptured="true"
				SpawnInsideBuilding="true"
				KillSpawnsOnDisabled="true">
				<Die
					DeathTypes="ALL" />
				<SpawnTemplate>AlliedWarFactoryRepairDrone</SpawnTemplate>
			</SpawnBehavior>
					
			<ProductionUpdate
				id="ModuleTag_ProductionUpdate"
				GiveNoXP="true"
				Type="WATERCRAFT"
				NumDoorAnimations="1"
				DoorOpeningTime="0.66s"
				DoorWaitOpenTime="3.00s"
				DoorCloseTime="0.66s">
				<GiveUpgradeToProduced>
					<UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_VETERAN"
						ProvidedUpgrade="Upgrade_CreationVeterancy_VETERAN" />
					<UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_ELITE"
						ProvidedUpgrade="Upgrade_CreationVeterancy_ELITE" />
					<UpgradeTranslate
						RequiredUpgrade="Upgrade_ProductionVeterancy_HEROIC"
						ProvidedUpgrade="Upgrade_CreationVeterancy_HEROIC" />
				</GiveUpgradeToProduced>
			</ProductionUpdate>
			<QueueProductionExitUpdate
				id="ModuleTag_QueueProduction"
				ExitDelaySeconds="0.07s">
				<UnitCreatePoint
					x="-25.0"
					y="0.0"
					z="0.0" />
				<NaturalRallyPoint
					x="90.0"
					y="0.0"
					z="0.0" />
			</QueueProductionExitUpdate>			
			<Physics
				id="ModuleTag_Physics"/>

			<BuildingDestructionBehavior
				id="ModuleTag_BuildingDestruction"
				FillerMesh="NEWSKIN_FILL">
				<DieMuxData
					DeathTypes="ALL" />
				<Item
					BonePrefix="BONE_CONTACT_POINT_"
					FX="SovietRefineryHit"
					CreationObject="AlliedBarracks_Debris"
					FlingVelocity="100.0"/>
			</BuildingDestructionBehavior>
			<FXListBehavior
				xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance" id="ModuleTag_FXList">
				<DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="SUICIDED" />
        <Event
					Index="onDeath"
					FX="FX_YU_Hotel02_Explode" />
				<Event
					Index="onDeath"
					FX="FX_LargeBuildingDieExplosionRumble" />
			</FXListBehavior>
      <FXListBehavior xai:joinAction="Replace" xmlns:xai="uri:ea.com:eala:asset:instance" id="ModuleTag_DieSound">
        <DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="SUICIDED" />
        <Event Index="onDeath" FX="FX_LargeBuildingDieExplosion" />
      </FXListBehavior>
			<CreateObjectDie
				id="ModuleTag_CreateObjectDie"
				CreationList="ABNavalYard_Die_OCL">
				<DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="SUICIDED" />
			</CreateObjectDie>
			<DestroyDie
				id="ModuleTag_Die">
				<DieMuxData
					DeathTypes="ALL"
					DeathTypesForbidden="SUICIDED" />
			</DestroyDie>	
			<BuildingBehavior
				id="ModuleTag_BuildingBehavior"
				NightWindowObject="LIGHT_01" />			
			<StructureUnpackUpdate
				id="ModuleTag_StructureUnpackUpdate"
				UnpackTime="2s" />
			<xi:include
				href="DATA:Includes/GenericBuildingRepair.xml" />
			<xi:include
				href="DATA:Includes/GenericEngineerContain.xml" />
			<xi:include
				href="DATA:Includes/GenericAlliedBuildingDestructionNoSpawn.xml" />
			<xi:include
				href="DATA:Includes/GenericALliedBuildingSuicideNoSpawn.xml" />
			<xi:include
				href="DATA:Includes/InfiltratorContainShip.xml" />
			<StancesBehavior
				id="ModuleTag_Stance"
				StanceTemplate="Generic" />

			<xi:include
				href="DATA:Includes/AlliedTechUpgradeReceiverLevel2.xml" />
			<xi:include
				href="DATA:Includes/AlliedTechUpgradeReceiverLevel3.xml" />
			<xi:include
				href="DATA:Includes/AlliedSubObjectsUpgradeTech2.xml" />
			<xi:include
				href="DATA:Includes/AlliedSubObjectsUpgradeTech3.xml" />
			<LUAEventList
				id="ModuleTag_LUAEventList"
				EventListName="AlliedBarracksFunctions" />
		</Behaviors>
		<AI>
			<AIUpdate
				id="ModuleTag_AIUpdate"
				AutoAcquireEnemiesWhenIdle="NO"
				StateMachine="StructureAIStateMachine"/>
		</AI>
		<Body>
			<ActiveBody
				id="ModuleTag_StructureBody"
				MaxHealth="2500" />
		</Body>
