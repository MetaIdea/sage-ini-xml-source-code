				ContactPointGeneration = "STRUCTURE" >
				<Offset
					x = "-23.4696"
					y = "0.0"
					z = "-15.107" />
			</Shape>
		</Geometry>
		<AudioArrayVoice>
			<AudioEntry
				Sound="ALL_Seaport_Select"
				AudioType="voiceSelect" />
		</AudioArrayVoice>
