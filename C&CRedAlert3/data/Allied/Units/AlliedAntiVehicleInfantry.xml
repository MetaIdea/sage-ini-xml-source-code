						Name="NPSkeleton_SKN" />
				</ModelConditionState>
				
				<ModelConditionState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
					<Model
						Name="NPRadiation" />
						<ParticleSysBone
						BoneName="None"
						FXParticleSystemTemplate="RadiationPuddle"
						FollowBone="true" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="RadiationSplash"
            FollowBone="true" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="RadiationLightSmall"
            FollowBone="true" />          
				</ModelConditionState>
				
				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					StateName="STATE_bored"
					AllowRepeatInRandomPick="true"
					Flags="RESTART_ANIM_WHEN_COMPLETE">
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIDA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIDB"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIDC"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="100"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFA"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFB"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFC"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFD"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFX"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFY"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					<Animation
						AnimationName="AUAntiVehicleInfantry_BIFZ"
						AnimationMode="ONCE"
						AnimationBlendTime="15"
						AnimationPriority="10"/>
					
					<Script>
						Prev = CurDrawablePrevAnimationState();
						if Prev == "STATE_Suppressed" then
						CurDrawableSetTransitionAnimState("TRANS_fromSuppressed")
						elseif Prev == "STATE_Parachuting" then
						CurDrawableSetTransitionAnimState("TRANS_fromParachuting")
						elseif Prev == "STATE_Selected" or Prev == "ATNB" or Prev == "STATE_Run" then
						CurDrawableSetTransitionAnimState("TRANS_SelectedToIdle")
						end
					</Script>
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="FRONTCRUSHED">
					<Animation
						AnimationName="AUAntiVehicleInfantry_CDTA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="BACKCRUSHED">
					<Animation
						AnimationName="AUAntiVehicleInfantry_CDTB"
						AnimationMode="ONCE" />
				</AnimationState>
			
				<!-- meta impact anims -->

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_FLAILING"
					StateName="STATE_stunned_flailing">
					<Animation
						AnimationName="AUAntiVehicleInfantry_FLYA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING SPLATTED">
					<Animation
						AnimationName="AUAntiVehicleInfantry_LNDA"
						AnimationMode="ONCE"
						AnimationBlendTime="15" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED_STANDING_UP">
					<Animation
						AnimationName="AUAntiVehicleInfantry_GTPA"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="STUNNED">
					<Animation
						AnimationName="AUAntiVehicleInfantry_LNDA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING OVER_WATER"
					StateName="STATE_OverWater">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SWMB"
						AnimationMode="LOOP" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="InfantryWaterWake"
            FollowBone="false" />          
						<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev ~= "STATE_OverWater" and Prev ~= "TRANS_ToWater"  then
						CurDrawableSetTransitionAnimState("TRANS_ToWater")
						end
					</Script>
					</AnimationState>
					
					<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="ENGAGED OVER_WATER">
					<Animation
						AnimationName="AUAntiVehicleInfantry_WADA"
						AnimationMode="LOOP" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="SmallShipWakeIdle"
            FollowBone="false" />          
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="OVER_WATER"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_OverWater">
					<Animation
						AnimationName="AUAntiVehicleInfantry_WADA"
						AnimationMode="LOOP" />
          <ParticleSysBone
            BoneName="None"
            FXParticleSystemTemplate="SmallShipWakeIdle"
            FollowBone="false" />          
					<!--Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_WaterBored" then
						CurDrawableSetTransitionAnimState("TRANS_WaterBoredToSelected")
						end
					</Script-->
				</AnimationState>

				<!-- parachuting anims -->

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="LAUNCHED"
					StateName="STATE_ParachuteLaunch">
					<Animation
						AnimationName="AUAntiVehicleInfantry_MCNA"
						AnimationMode="LOOP" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="PARACHUTING"
					StateName="STATE_Parachuting">
					<Animation
						AnimationName="AUAntiVehicleInfantry_MCNC"
						AnimationMode="LOOP" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_ParachuteLaunch" then
						CurDrawableSetTransitionAnimState("TRANS_toParachuting")
						end
					</Script>
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_toParachuting" >
					<Animation
						AnimationName="AUAntiVehicleInfantry_MCNB"
						AnimationMode="ONCE" />
				</AnimationState>
				<AnimationState
					ParseCondStateType="PARSE_TRANSITION"
					StateName="TRANS_fromParachuting" >
					<Animation
						AnimationName="AUAntiVehicleInfantry_MCND"
						AnimationMode="ONCE" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED DYING"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SDIA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<!-- radiation death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5 OVER_WATER">
					<Animation
						AnimationName="NPRadiation_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<!-- radiation death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_5">
					<Animation
						AnimationName="NPRadiation_DIEA"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- sword death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_4">
					<Animation
						AnimationName="AUAntiVehicleInfantry_DTKA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="AUAntiVehicleInfantry_DTKB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- fire death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_3">
					<Animation
						AnimationName="AUAntiVehicleInfantry_DTFA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="AUAntiVehicleInfantry_DTFB"
						AnimationMode="ONCE" />
					<ParticleSysBone
						BoneName="HIPS"
						FXParticleSystemTemplate="InfantryFireDeath"
						FollowBone="true" />
				</AnimationState>


				<!-- prism/laser death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_2">
					<Animation
						AnimationName="AUAntiVehicleInfantry_DTPA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="AUAntiVehicleInfantry_DTPB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- tesla death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING DEATH_1">
					<Animation
						AnimationName="NPSkeleton_DIEA"
						AnimationMode="ONCE" />
						<ParticleSysBone
						BoneName="HIPS"
						FXParticleSystemTemplate="Soviet_TeslaDeath_Spark"
						FollowBone="true" />
				</AnimationState>
				
					<!-- dying when running -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING_WASMOVING">
					<Animation
						AnimationName="AUAntiVehicleInfantry_RNDA"
						AnimationMode="ONCE" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_RNDB"
						AnimationMode="ONCE" />
				</AnimationState>

				<!-- default death -->
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="DYING">
					<Animation
						AnimationName="AUAntiVehicleInfantry_DTBA"
						AnimationMode="ONCE" />
						<Animation
						AnimationName="AUAntiVehicleInfantry_DTBB"
						AnimationMode="ONCE" />
				</AnimationState>
				
				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="AFFECTED_BY_EMP">
					<Animation
						AnimationName="AUAntiVehicleInfantry_CWRY"
						AnimationMode="LOOP" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_CWRZ"
						AnimationMode="LOOP" />
					<ParticleSysBone
							BoneName="NONE"
							FXParticleSystemTemplate="Allied_DogStun01"
							FollowBone="true" />
					<ParticleSysBone
							BoneName="NONE"
							FXParticleSystemTemplate="Allied_DogStunGlows01"
							FollowBone="true" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED MOVING"
					StateName="STATE_Suppressed"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SMVA"
						AnimationMode="LOOP" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED USING_WEAPON_A"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SATA"
						AnimationMode="LOOP" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="SUPPRESSED"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					StateName="STATE_Suppressed">
					<Animation
						AnimationName="AUAntiVehicleInfantry_SIDA"
						AnimationMode="ONCE"
						AnimationPriority="5" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev ~= "STATE_Suppressed" and Prev ~= "TRANS_toSuppressed" then
						CurDrawableSetTransitionAnimState("TRANS_toSuppressed")
						end
					</Script>
				</AnimationState>
				
				<AnimationState
					  ParseCondStateType="PARSE_NORMAL"
					  ConditionsYes="MOVING FIRING_A"
					  Flags="RANDOMSTART">
					<Animation
						AnimationName="AUAntiVehicleInfantry_ATRA"
						AnimationMode="LOOP"
						Distance="75" />
				</AnimationState>

				<AnimationState
					ParseCondStateType="PARSE_NORMAL"
					ConditionsYes="MOVING"
					StateName="STATE_Run"
					Flags="RANDOMSTART">
					<Animation
						AnimationName="AUAntiVehicleInfantry_RUNA"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="40" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_RUNB"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="40" />
					<Animation
						AnimationName="AUAntiVehicleInfantry_RUNC"
						AnimationMode="LOOP"
						AnimationBlendTime="10"
						Distance="40" />
					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_OverWater" or Prev == "TRANS_ToWater" then
						CurDrawableSetTransitionAnimState("TRANS_FromWater")
						end
					</Script>
